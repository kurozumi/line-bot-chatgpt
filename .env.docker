# Docker環境用の環境変数設定ファイル
# このファイルをコピーして.env.localとして使用してください

# Database
POSTGRES_VERSION=16
POSTGRES_DB=app
POSTGRES_USER=app
POSTGRES_PASSWORD=!ChangeMe!

# LINE Bot
LINE_ACCESS_TOKEN=your_line_access_token_here

